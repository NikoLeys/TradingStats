<Window x:Class="TradingStats.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TradingStats"
        mc:Ignorable="d"
        Title="TradingStats" Height="450" Width="800" FontFamily="Times New Roman" Icon="Image/fixprib.jpg">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>

        <Image Source="Image/fixprib.jpg" HorizontalAlignment="Left"></Image>
        <TextBlock Text="TradingStats" HorizontalAlignment="Center" FontSize="30" Margin="0 5 0 0"></TextBlock>
        <TextBlock Text="Фиксируем прибыль" HorizontalAlignment="Center" FontSize="20" Margin="0 35 0 0"></TextBlock>
        <Button Content="Назад" Name="BackBtn" Width="150" Height="30" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5" Click="BackBtn_Click" Background="Black" Foreground="White" FontSize="20"></Button>
        
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        
    </Grid>
</Window>
